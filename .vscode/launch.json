{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Launch localhost",
          "type": "chrome",
          "request": "launch",
          "url": "http://localhost:4200",
          "webRoot": "${workspaceFolder}/wwwroot",
          "userDataDir": "D:/chrome-dev-profile"
      },
      {
          "name": "Launch index.html (disable sourcemaps)",
          "type": "chrome",
          "request": "launch",
          "sourceMaps": false,
          "file": "${workspaceFolder}/index.html"
      },
  ]
}
